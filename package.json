		"@types/react-dom": "^18.0.9",
		"@types/react-router-dom": "^5.3.3",
		"autoprefixer": "^10.4.13",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"electron": "^22.0.0",
		"electron-builder": "^21.2.0",
		"postcss": "^8.4.19",
		"prettier": "^2.8.0",
		"react-scripts": "^5.0.1",
		"rimraf": "^5.0.7",
		"tailwindcss": "3.2.4",
		"taze": "^0.8.4",
		"typescript": "^4.9.3",
		"wait-on": "^6.0.1"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.8.2",
		"@electron/remote": "^2.0.8",
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.11.5",
		"@linways/table-to-excel": "^1.0.4",
		"auto-launch": "^5.0.5",
		"axios": "^1.7.2",
		"electron-is-dev": "^2.0.0",
		"electron-packager": "^17.1.2",
		"electron-react-devtools": "^0.5.3",
		"electron-reloader": "^1.2.3",
		"electron-updater": "^5.3.0",
		"electron-winstaller": "^5.3.1",
		"framer-motion": "^11.2.11",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-export-table-to-excel": "^1.0.6",
		"react-icons": "^4.7.1",
		"react-query": "^3.39.3",
		"react-router-dom": "^6.4.4",
		"react-use": "^17.4.0",
		"sass": "^1.77.6"
	},
	"homepage": "./",
	"main": "public/electron.js",
	"scripts": {
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"dev": "concurrently \"npm run react:dev\" \"npm run electron:wait\"",
		"electron:wait": "wait-on tcp:3000 && npm run electron:dev",
		"prebuild": "rimraf build && rimraf dist",
		"react:build": "yarn prebuild && react-scripts build",
		"react:dev": "cross-env BROWSER=none react-scripts start",
		"electron:build": "electron-builder build --win --publish never",
		"electron:build1": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
		"electron:deploy": "electron-builder build --win --publish always",
		"electron:dev": "electron .",
		"build": "npm run react:build && npm run electron:build",
		"deploy": "npm run react:build && npm run electron:deploy",
		"format": "prettier --write .",
		"update": "taze latest -w",
		"package-win": "rimraf build && rimraf dist && yarn react:build && electron-packager . Manage --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Manage\""
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
